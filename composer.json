{
    "name": "piotrminkina/workflow-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony2 Workflow Bundle to define workflows to handle the volatile and changing business processes",
    "authors": [
        {
            "name": "Piotr Minkina",
            "email": "projekty@piotrminkina.pl",
            "homepage": "http://www.piotrminkina.pl"
        }
    ],
    "autoload": {
        "psr-0": { "PMD\\WorkflowBundle": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/collections": "1.2.*",
        "symfony/http-kernel": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "PMD/WorkflowBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
